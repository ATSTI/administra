select c.CODCLIENTE, c.NOMECLIENTE, c.RAZAOSOCIAL, c.TIPOFIRMA, c.CNPJ, c.INSCESTADUAL,
  ed.LOGRADOURO, ed.NUMERO, (ed.DDD || '-' || ed.TELEFONE) FONE1, (ed.DDD1 || '-' || ed.TELEFONE1) FONE2, 
  (ed.DDD2 || '-' || ed.TELEFONE2) FONE3, ed.BAIRRO, ed.CD_IBGE, ed.CIDADE, ed.COMPLEMENTO, ed.DADOSADICIONAIS, 
  ed.E_MAIL, ed.UF  
  from clientes c , ENDERECOCLIENTE ed 
 where c.CODCLIENTE = ed.CODCLIENTE