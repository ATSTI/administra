uses UDm;

{$R *.dfm}

procedure TDMEscola.cdsAlunosNewRecord(DataSet: TDataSet);
begin
  cdsAlunosCNPJ.EditMask := '000.000.000-00';
  cdsAlunosTIPOFIRMA.AsInteger := 0;
  cdsAlunosSEGMENTO.Value:=1;
  cdsAlunosANOLETIVO.AsString := IntToStr(anoativo);
  cdsAlunosSITUACAOESCOLAR.AsString := 'M';
  if (not dm.scds_usuario_proc.Active) then
     dm.scds_usuario_proc.Open;
  dm.scds_usuario_proc.Locate('CODUSUARIO','1', [loCaseInsensitive]);
  cdsAlunosCODUSUARIO.Value := 1;
  dm.scds_usuario_proc.Close;
  cdsAlunosSTATUS.Value:=1;
  cdsAlunosREGIAO.Value:=1;
  cdsAlunosDATACADASTRO.Value:=Date;
  cdsAlunosSITUACAOESCOLAR.AsString := 'M';
  cdsAlunosANOLETIVO.AsString := IntToStr(anoativo);
  if not (cdsEndAluno.Active) then
     cdsEndAluno.open;
  cdsEndAluno.Append;
  cdsEndAlunoUF.AsString := 'SP';
end;

procedure TDMEscola.DataModuleCreate(Sender: TObject);
begin
   cdsEscola.Open;
   anoativo := cdsEscolaANOLETIVO.AsInteger;
   cdsEscola.Close;
end;

procedure TDMEscola.cdsAlunosBeforePost(DataSet: TDataSet);
begin
    if (cdsAlunos.State in [dsInsert]) then
    begin
      if dm.c_6_genid.Active then
        dm.c_6_genid.Close;
      dm.c_6_genid.CommandText := 'SELECT CAST(GEN_ID(GEN_CLI, 1) as INTEGER) AS CODIGO FROM RDB$DATABASE';
      dm.c_6_genid.Open;
      cdsAlunosCODCLIENTE.AsInteger := dm.c_6_genidCODIGO.AsInteger;
      if cdsAlunosRA.IsNull then
        cdsAlunosRA.AsString := IntToStr(dm.c_6_genidCODIGO.AsInteger);
      dm.c_6_genid.Close;
      cdsAlunosRAZAOSOCIAL.AsString := cdsAlunosNOMECLIENTE.AsString;
    end;
    if (cdsAlunos.State in [dsEdit]) then
       cdsAlunosRAZAOSOCIAL.AsString := cdsAlunosNOMECLIENTE.AsString;
end;

procedure TDMEscola.cdsEndAlunoBeforePost(DataSet: TDataSet);
begin
     if (DMEscola.cdsEndAluno.State in [dsInsert]) then
     begin
        if dm.c_6_genid.Active then
          dm.c_6_genid.Close;
        dm.c_6_genid.CommandText := 'SELECT CAST(GEN_ID(GEN_END_CLI, 1) as INTEGER) AS CODIGO FROM RDB$DATABASE';
        dm.c_6_genid.Open;
        cdsEndAlunoCODENDERECO.AsInteger := dm.c_6_genidCODIGO.AsInteger;
        dm.c_6_genid.Close;
        cdsEndAlunoCODCLIENTE.AsInteger := cdsAlunosCODCLIENTE.AsInteger;
        cdsEndAlunoUF.AsString := 'SP';
        cdsEndAlunoTIPOEND.AsInteger := 0;
     end;
end;

end.
