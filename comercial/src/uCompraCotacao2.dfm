  object sqlPedido: TSQLQuery
    MaxBlobSize = -1
    Params = <
      item
        DataType = ftInteger
        Name = 'FORNEC'
        ParamType = ptUnknown
      end>
    SQL.Strings = (
      
        'select m.codmovimento, m.STATUS from MOVIMENTO m, MOVIMENTODETAL' +
        'HE md '
      '         where m.CODMOVIMENTO = md.CODMOVIMENTO      '
      '           and md.BAIXA is null '
      '           and m.CODNATUREZA       = 5'
      '           and m.STATUS                    = 0'
      '           and m.CODFORNECEDOR = :FORNEC'
      '           order by m.CODMOVIMENTO ')
    SQLConnection = DM.sqlsisAdimin
    Left = 64
    Top = 200
  end
