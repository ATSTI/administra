      '           ,ENDE.LOGRADOURO'#13#10'           ,ENDE.CIDADE'#13#10'          ' +
      ' ,ENDE.BAIRRO'#13#10'           ,ENDE.UF'#13#10'           ,ENDE.CEP '#13#10'     ' +
      '     ,ENDE.COMPLEMENTO      '#13#10'           '#13#10'      '#13#10'from CLIENTES' +
      ' CLI'#13#10#13#10'LEFT OUTER JOIN  ENDERECOCLIENTE ENDE ON ENDE.CODCLIENTE' +
