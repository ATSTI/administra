  object sds_Mov_Det: TSQLDataSet
    CommandText = 
      'select movd.CODDETALHE'#13#10'        , movd.CODMOVIMENTO'#13#10'        , m' +
      'ovd.CODPRODUTO'#13#10'        , movd.ICMS'#13#10'        , movd.PRECO'#13#10'     ' +
      '   , movd.QUANTIDADE'#13#10'        , movd.QTDE_ALT'#13#10'        , movd.UN' +
      #13#10'        , movd.BAIXA'#13#10'        , movd.CONTROLE'#13#10'        , movd.' +
      'COD_COMISSAO '#13#10'        , movd.VALTOTAL'#13#10'        , movd.PRECOCUST' +
      'O'#13#10'        , movd.DESCPRODUTO'#13#10'        , movd.LOTE'#13#10'        , mo' +
      'vd.CODAUTORIZACAO'#13#10'        , movd.STATUS'#13#10'        , movd.CODALMO' +
      'XARIFADO'#13#10'        , movd.PERIODOINI'#13#10'        , movd.PERIODOFIM'#13#10 +
      '        , (case when (movd.PERIODOINI is not null) then udf_IncM' +
      'inute(movd.PERIODOINI, prod.QTDE_PCT)  end) as Horatermino '#13#10'   ' +
      '     , prod.CODPRO'#13#10'        , prod.PRODUTO'#13#10'        , prod.VALOR' +
      'UNITARIOATUAL'#13#10'        , prod.QTDE_PCT'#13#10'        , prod.CONTA_DES' +
      'PESA  '#13#10'        , prod.COD_BARRA '#13#10'        , prod.LOCALIZACAO '#13#10 +
      '        , prod.ESTOQUEATUAL '#13#10'        , pla.NOME '#13#10'        , cli' +
      '.NOMECLIENTE '#13#10'from MOVIMENTODETALHE movd '#13#10'inner join PRODUTOS ' +
      'prod on prod.CODPRODUTO=movd.CODPRODUTO '#13#10'left outer join PLANO ' +
      'pla on pla.CODIGO = movd.CODALMOXARIFADO '#13#10'inner join MOVIMENTO ' +
      'm on m.CODMOVIMENTO = movd.CODMOVIMENTO '#13#10'inner join CLIENTES cl' +
      'i on cli.CODCLIENTE = m.CODCLIENTE '#13#10'where movd.CODALMOXARIFADO ' +
      '= :pCOD '#13#10'and movd.PERIODOFIM is null'#13#10
    MaxBlobSize = -1
    Params = <
      item
        DataType = ftInteger
        Name = 'pCOD'
        ParamType = ptInput
      end>
    SQLConnection = DM.sqlsisAdimin
    Left = 95
    Top = 215
