        'select comp.NOTAFISCAL,       '#13#10'           comp.DATAVENCIMENTO, ' +
        #13#10'          comp.VALOR,'#13#10'          comp.DESCONTO,'#13#10'          (co' +
        'mp.VALOR + comp.DESCONTO ) as total,'#13#10'          comp.DATAVENCIME' +
        'NTO,'#13#10'          forn.RAZAOSOCIAL,      '#13#10'          prod.CODPRO, ' +
        #13#10'          prod.PRODUTO,          '#13#10'          movd.VALTOTAL,'#13#10' ' +
