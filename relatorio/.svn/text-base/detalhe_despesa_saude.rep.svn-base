        '       cli.nomecliente,'#13#10'          cli.cod_faixa '#13#10#13#10'   from COM' +
        'PRA  comp '#13#10' '#13#10#13#10'   left outer join MovimentoDetalhe movd on mov' +
        'd.codMovimento = comp.CodMovimento '#13#10'   left outer join Produtos' +
        ' prod on prod.CodProduto = movd.CodProduto'#13#10'   left outer join F' +
        'ornecedor forn on forn.CodFornecedor = comp.codFornecedor'#13#10' '#13#10'  ' +
        'left outer join Clientes cli on cli.codcliente = movd.codalmoxar' +
        'ifado'#13#10#13#10#13#10#13#10#13#10#13#10#13#10
    end
    item
      Alias = 'IMPCAB'
      DatabaseAlias = 'SISADMIN'
