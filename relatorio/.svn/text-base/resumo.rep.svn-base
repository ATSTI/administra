  DataInfo = <
    item
      Alias = 'IMPR'
      DatabaseAlias = 'SISADMIN'
      SQL = 
        'SELECT  MOV.CODPRODUTO '#13#10'               ,MOV.UN '#13#10'              ' +
        ' ,MOV.VALTOTAL AS VALORTOTAL '#13#10'               ,PRO.PRODUTO '#13#10'   ' +
        '            ,ENDE.LOGRADOURO'#13#10'              ,ENDE.CIDADE'#13#10'      ' +
        '        ,ENDE.BAIRRO'#13#10'              ,ENDE.UF'#13#10'              ,END' +
        'E.CEP'#13#10'              ,ENDE.DDD'#13#10'              ,ENDE.TELEFONE'#13#10'  ' +
        '            ,CAST(VEN.CODCLIENTE AS VARCHAR(4)) || '#39'-'#39' || CLI.NO' +
        'MECLIENTE as CLI '#13#10'              ,VEN.DATAVENDA'#13#10'              ,' +
        'VEN.DATAVENCIMENTO'#13#10'              ,VEN.CODMOVIMENTO'#13#10'           ' +
        '   ,VEN.VALOR'#13#10'              ,VEN.DESCONTO '#13#10'              ,CLI.' +
        'CNPJ'#13#10'             ,CLI.TIPOFIRMA'#13#10'             ,MOV.COD_COMISSA' +
        'O'#13#10'FROM VENDA VEN '#13#10'INNER JOIN MOVIMENTODETALHE MOV ON MOV.CODMO' +
        'VIMENTO = VEN.CODMOVIMENTO'#13#10'INNER JOIN CLIENTES CLI ON CLI.CODCL' +
        'IENTE = VEN.CODCLIENTE'#13#10'LEFT JOIN  ENDERECOCLIENTE ENDE ON ENDE.' +
        'CODCLIENTE = CLI.CODCLIENTE '#13#10'INNER JOIN PRODUTOS PRO ON PRO.COD' +
        'PRODUTO = MOV.CODPRODUTO '#13#10'WHERE (VEN.DATAVENCIMENTO between :DA' +
        'TAINI and :DATAFIN) and ((VEN.CODCLIENTE = :CODCLI ) or (:CODCLI' +
        ' = 100000 )) and (VEN.CODCLIENTE > 0) GROUP BY VEN.CODCLIENTE ,V' +
        'EN.DATAVENCIMENTO,VEN.DATAVENDA , MOV.CODPRODUTO    '#13#10'          ' +
        '     ,MOV.UN  '#13#10'              , MOV.valtotal'#13#10'               ,PR' +
        'O.PRODUTO  '#13#10'               ,ENDE.LOGRADOURO '#13#10'              ,EN' +
        'DE.CIDADE '#13#10'              ,ENDE.BAIRRO '#13#10'              ,ENDE.UF ' +
        #13#10'              ,ENDE.CEP '#13#10'              ,ENDE.DDD '#13#10'          ' +
        '    ,ENDE.TELEFONE '#13#10'              ,VEN.CODMOVIMENTO '#13#10'         ' +
        '     ,VEN.VALOR '#13#10'              ,VEN.NOTAFISCAL '#13#10'              ' +
        ',VEN.SERIE '#13#10'              ,VEN.DESCONTO '#13#10'              ,CLI.NO' +
        'MECLIENTE '#13#10'              ,CLI.CNPJ '#13#10'             ,CLI.TIPOFIRM' +
        'A'#13#10'             ,MOV.COD_COMISSAO;     '
    end
    item
      Alias = 'IMPCAB'
      DatabaseAlias = 'SISADMIN'
      SQL = 
        'SELECT EMPRESA || '#39' - '#39' ||RAZAO as EMP, BAIRRO, CIDADE || '#39' - '#39' ' +
        '|| UF || '#39' - '#39' || CEP as local, '#13#10#39'FONE: (19)-'#39' || FONE || '#39'/'#39' |' +
        '| FONE_1 || '#39'/'#39' || FONE_2 || '#39' - '#39' || WEB || '#39' - '#39' || e_mail as ' +
        'comunica '#13#10'FROM EMPRESA'
    end
