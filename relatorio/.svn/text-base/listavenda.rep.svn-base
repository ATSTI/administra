  DataInfo = <
    item
      Alias = 'IMP'
      DatabaseAlias = 'SISADMIN'
      SQL = 
        'select mov.CODCLIENTE, mov.CODMOVIMENTO, '#13#10'      mov.CODNATUREZA' +
        ', mov.DATAMOVIMENTO, mov.STATUS,'#13#10'      SUM(movd.QUANTIDADE * mo' +
        'vd.PRECO)  as PRECO,'#13#10'      cli.NOMECLIENTE, '#13#10'      nat.DESCNAT' +
        'UREZA, mov.CODFORNECEDOR, forn.NOMEFORNECEDOR, ven.NOTAFISCAL, v' +
        'en.SERIE, ven.VALOR, ven.APAGAR, ven.DATAVENDA   '#13#10'      from MO' +
        'VIMENTO mov left outer join CLIENTES cli on cli.CODCLIENTE =  mo' +
        'v.CODCLIENTE  inner join NATUREZAOPERACAO nat '#13#10'      on nat.COD' +
        'NATUREZA  = mov.CODNATUREZA left outer join FORNECEDOR forn '#13#10'  ' +
        '    on forn.CODFORNECEDOR = mov.CODFORNECEDOR left outer join VE' +
        'NDA ven '#13#10'      on ven.CODMOVIMENTO = mov.CODMOVIMENTO '#13#10'left ou' +
        'ter join MOVIMENTODETALHE movd '#13#10'      on movd.CODMOVIMENTO = mo' +
        'v.CODMOVIMENTO '#13#10'     group by '#13#10'       mov.CODCLIENTE, mov.CODM' +
        'OVIMENTO, '#13#10'      mov.CODNATUREZA, mov.DATAMOVIMENTO, mov.STATUS' +
        ','#13#10'      cli.NOMECLIENTE, '#13#10'      nat.DESCNATUREZA, mov.CODFORNE' +
        'CEDOR, forn.NOMEFORNECEDOR, ven.NOTAFISCAL, ven.SERIE, ven.VALOR' +
        ', ven.APAGAR, ven.DATAVENDA '
    end
    item
      Alias = 'EMPRESA'
