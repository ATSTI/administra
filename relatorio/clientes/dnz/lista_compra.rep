  DataInfo = <
    item
      Alias = 'IMPR'
      DatabaseAlias = 'SGE'
      SQL = 
        'SELECT  MOV.CODPRODUTO  ,SUM(MOV.QUANTIDADE) AS QTDE  ,SUM(MOV.P' +
        'RECO) AS PCO '#13#10'               ,MOV.UN  ,SUM(MOV.QUANTIDADE *  MO' +
        'V.PRECO) AS VALORTOTAL  ,PRO.PRODUTO , pro.codPro'#13#10'             ' +
        '  ,ENDE.LOGRADOURO ,ENDE.CIDADE ,ENDE.BAIRRO ,ENDE.UF ,ENDE.CEP ' +
        ',ENDE.DDD'#13#10'              ,ENDE.TELEFONE ,CLI.NOMEFORNECEDOR as C' +
        'LI  ,VEN.DATAMOVIMENTO ,VEN.CODMOVIMENTO ,CLI.CNPJ,'#13#10'           ' +
        '   VEN.DATA_ENTREGA, VEN.PRAZO_PAGAMENTO'#13#10'FROM MOVIMENTO VEN '#13#10'I' +
        'NNER JOIN MOVIMENTODETALHE MOV ON MOV.CODMOVIMENTO = VEN.CODMOVI' +
        'MENTO'#13#10'INNER JOIN FORNECEDOR CLI ON CLI.CODFORNECEDOR = VEN.CODF' +
        'ORNECEDOR'#13#10'INNER JOIN  ENDERECOFORNECEDOR ENDE ON ENDE.CODFORNEC' +
        'EDOR = CLI.CODFORNECEDOR '#13#10'INNER JOIN PRODUTOS PRO ON PRO.CODPRO' +
        'DUTO = MOV.CODPRODUTO '#13#10'WHERE VEN.CODMOVIMENTO = :PCOMPRA and en' +
        'de.tipoend = 0 GROUP BY   MOV.CODPRODUTO    '#13#10'               ,MO' +
        'V.UN   ,PRO.PRODUTO  , pro.codPro ,ENDE.LOGRADOURO '#13#10'           ' +
        '   ,ENDE.CIDADE  ,ENDE.BAIRRO  ,ENDE.UF  ,ENDE.CEP  ,ENDE.DDD  ,' +
        'ENDE.TELEFONE '#13#10'              ,VEN.CODFORNECEDOR ,VEN.DATAMOVIME' +
        'NTO ,VEN.CODMOVIMENTO  ,CLI.NOMEFORNECEDOR'#13#10'              ,CLI.C' +
        'NPJ ,'#13#10'              VEN.DATA_ENTREGA, VEN.PRAZO_PAGAMENTO'
    end
    item
      Alias = 'IMPCAB'
      DatabaseAlias = 'SGE'
      SQL = 
        'SELECT EMPRESA as EMP, ENDERECO || '#39' - '#39' || BAIRRO as bairro, CI' +
